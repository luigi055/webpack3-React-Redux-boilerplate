{
  "name": "web-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate using webpack2",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "SET NODE_ENV=development& webpack",
    "build-watch": "SET NODE_ENV=development& webpack -d -w",
    "prod": "SET NODE_ENV=production& webpack -p",
    "test": "mocha **/*.test.js",
    "text-watch": "nodemon --exec \"yarn test\""
  },
  "repository": {},
  "author": "Pedro La Rosa",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "^0.10.0",
    "handlebars-loader": "^1.4.0",
    "html-loader": "^0.4.4",
    "image-webpack-loader": "^3.2.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "pixrem": "^3.0.2",
    "postcss-loader": "^1.3.2",
    "postcss-vmin": "^2.0.0",
    "postcss-will-change": "^1.1.0",
    "resolve-url-loader": "^2.0.0",
    "sass-loader": "^6.0.2",
    "script-loader": "^0.7.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "express": "^4.14.1",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.3.0",
    "handlebars": "^4.0.6",
    "jquery": "^3.1.1"
  }
}
