{
  "name": "reactRedux-webpack2-boilerplate",
  "version": "1.0.0",
  "description": "Webpack 2 Boilerplate for React + Redux",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "SET NODE_ENV=development& webpack",
    "build-watch": "SET NODE_ENV=development& webpack -d -w",
    "prod": "SET NODE_ENV=production& webpack -p",
    "test": "mocha --compilers js:babel-core/register --require ./src/tests/testHelpers.js --recursive ./src/tests",
    "test-watch": "yarn run test -- --watch"
  },
  "repository": {},
  "author": "Pedro La Rosa",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "0.27.3",
    "css-mqpacker": "^5.0.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "pixrem": "^3.0.2",
    "postcss-loader": "^1.3.2",
    "postcss-vmin": "^2.0.0",
    "postcss-will-change": "^1.1.0",
    "react-addons-test-utils": "^15.4.2",
    "resolve-url-loader": "^2.0.0",
    "sass-loader": "^6.0.2",
    "script-loader": "^0.7.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
